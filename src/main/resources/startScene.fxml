<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:id = "startPane"
            fx:controller="Controller">

    <padding> <Insets topRightBottomLeft="70"/> </padding>

    <center>
        <HBox fx:id = "buttonBox" spacing= "175">
            <Button fx:id = "serverChoice" text = "Server" onAction = "#server"/>
            <Button fx:id = "clientChoice" text = "Client" onAction = "#client"/>
        </HBox>
    </center>
</BorderPane>
